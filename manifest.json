{
	"manifest_version": 2,

	"name": "PlayerTimer",
	"description": "Dual timer",
	"version": "0.4",

	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},

	"browser_action": {
		"default_icon": "images/icon16.png",
		"default_popup": "popup.html"
	},
	"permissions": [
    "activeTab"
  ],
	"background": {
		"page": "background.html"
	}
}